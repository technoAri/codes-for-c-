#include <iostream>
using namespace std;
 
int main()
{
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
  {
    	string str;
    	cin>>str;
    	int size=str.length();
  	int count=0,j=0;
    	while(j<size)
    	{
    	
    		if(str[j]=='a' || str[j]=='e'  || str[j]=='i'  || str[j]=='o'  || str[j]=='u' )
    			count++;
    			j++;
    	}
    	count=count+3;//www.
    	cout<<size-count<<"/"<<size<<endl;;
    }
    return 0;
}
