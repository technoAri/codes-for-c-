#include <iostream>
using namespace std;

int main()
{
    int L,N,W,H;
    cin>>L;
    cin>>N;
   for(int i=0;i<N;i++)
   {
    cin>>W;
    cin>>H;
    if(W<L||H<L)
    {
    	cout<<"\n"<<"UPLOAD ANOTHER";
    }
    else if(W==H)
    {
    	cout<<"\n"<<"ACCEPTED";
    }
    else
    	cout<<"\n"<<"CROP IT";
   }
    return 0;
}
